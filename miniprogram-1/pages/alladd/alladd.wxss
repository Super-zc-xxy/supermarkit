/* pages/alladd/alladd.wxss */
page{
  background:rgb(243,243,243)
}
.addres_info{
  width: 80%;
  margin: 20rpx auto;
  padding: 40rpx;
  background: white;
  border-radius: 10rpx;
}
.info_header{
  display: flex;
  width: 100%;
  font-size: 30rpx;
  font-weight: 700;
}
.info_name{
  margin-right: 30rpx;
}
.info_addres{
  margin-top: 10rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  font-size: 20rpx;
  color: #999;
}
.hr{
  width: 100%;
  height: 2rpx;
  background: rgb(243,243,243);
  margin-top: 20rpx;
}
.moren{
  margin-top: 10rpx;
  font-size: 20rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.delet{
  margin-left: 290rpx;
}